meta {
  name: Requete9
  type: graphql
  seq: 11
}

post {
  url: 127.0.0.1:8055/graphql/
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{DIRECTUS_TOKEN}}
}

body:graphql {
  query PraticiensParVille($ville: String!) {
    praticien(filter: { ville: { _eq: $ville } }) {
      id
      nom
      prenom
      telephone
      ville
      specialite {
        libelle
      }
    }
  }
  
}

body:graphql:vars {
  {
    "ville": "Paris"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
