meta {
  name: Requete4
  type: graphql
  seq: 6
}

post {
  url: 127.0.0.1:8055/graphql/
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{DIRECTUS_TOKEN}}
}

body:graphql {
  query PraticiensStructure {
    praticiens(filter: { ville: { _eq: "Paris" } }) {
      id
      nom
      prenom
      telephone
      ville
      specialite {
        libelle
      }
      structure {
        nom
        ville
      }
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
